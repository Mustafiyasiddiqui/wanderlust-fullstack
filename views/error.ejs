<% layout("/layouts/boilerplate") %>

    <div class="row">
        <div class="alert alert-danger mt-5" role="alert">
            <h4 class="offset-2">
                <svg class="bi flex-shrink-0 me-2" role="img" aria-label="Danger:">
                    <use xlink:href="#exclamation-triangle-fill" />
                </svg>
                <%=err.message%>
            </h4>
            <p>
                <%= err.stack %>
            </p>
        </div>

    </div>